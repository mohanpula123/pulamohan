# map function
def multiply_by2(item):
  return item**2
print(list(map(multiply_by2,(1,2,3,4))))

# filter function
def check_odd(item):
  return item%2!=0
print(list(filter(check_odd,(1,2,3,4,8,10))))

#zip function
list1 = [1,2,3,4,5]
list2 = [6,7,8,9,10]
print(list(zip(list1,list2)))

#reduce function
from functools import reduce
def accumulator(acc,it):
  return acc+it
print(reduce(accumulator,(1,2,3,4)))

