for i in range(100):
    print(i)


for i in range(0,50,3):
     print(i)

#reverse
for i in range(50,0,-5):
    print(i)
for i in range(2):
    print(list(range(20))

#enumerate

for i,char in enumerate "hello":
      print(i,char)
for i,char in enumerate ([1,2,3,4,5,6]:
      print(i,char)

for i,char in (range(list(100))):
     if char==50:
        print(i,char)

