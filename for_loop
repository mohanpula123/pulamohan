for i in 'zero to hero;:
     print(i)

#for loop list
for i in [1,2,3,4]:
    print(i)

#for loop to matrix
for i in [1,2,3,4,5,6]:
    for j in [11,22,33,44]:
        print(i,j)

#for loop dict
user = {
      'name': 'mohan',
      'age': 24,
      'gen':'male'
      }
 for i in user.items():
     print(i)

 for i in user.values():
     print(i)

 for i in user.key():
      print(i)
